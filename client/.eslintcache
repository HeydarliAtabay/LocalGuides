} />\r\n                    {/* <span className=\"right-message\">\r\n                        <label>\r\n                            Hello...\r\n                        </label>\r\n                        <img alt=\"Profile\" src={photo} />\r\n                    </span>\r\n                    <span className=\"left-message\">\r\n                        <img alt=\"Profile\" src={photo} />\r\n                        <label>\r\n                            Hii \r\n                        </label>\r\n                    </span> */}\r\n                </div>\r\n               \r\n                {/* <span className=\"type-message\">\r\n                    <input type=\"text\" placeholder=\"type message\" />\r\n                    <label>{send}</label>\r\n                </span> */}\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default Chat;","C:\\Users\\519\\Desktop\\Human Computer Interaction\\local-guides-code\\client\\src\\Components\\Filter.js",["122","123"],"C:\\Users\\519\\Desktop\\Human Computer Interaction\\local-guides-code\\client\\src\\Components\\CreateTrip.js",["124","125","126","127","128"],"import React, { Component } from 'react'\r\nimport PropTypes from 'prop-types'\r\nimport GuideItem from './GuideItem'\r\nimport \"./CSS/CreateTrip.css\"\r\nimport { Button,FormCheck,Form } from 'react-bootstrap'\r\nimport Slider from '@material-ui/core/Slider';\r\n\r\n\r\nexport default class CreateTrip extends Component {\r\n    static propTypes = {\r\n        prop: PropTypes\r\n    }\r\n\r\n    render() {\r\n       \r\n        return (\r\n            <div className=\"guidelist\">\r\n            <h3 className=\"title\">Create a trip</h3>\r\n            <div className=\"requestcont\">\r\n\r\n            <h3 style={{marginLeft:20}}>Price range</h3>\r\n      \r\n\r\n            </div>\r\n           \r\n               \r\n               \r\n           \r\n            \r\n        </div>\r\n        )\r\n    }\r\n}\r\n","C:\\Users\\519\\Desktop\\Human Comput